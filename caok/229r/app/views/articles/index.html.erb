<h1>Articles</h1>

<div id="articles">
  <% @articles.each do |article| %>
    <h2><%= link_to article.name, article %></h2>
    <p class="info">Published on <%= article.created_at.strftime("%B %e, %Y") %></p>
    <%= simple_format article.content %>
  <% end %>
</div>

<p><%= link_to "New Article", new_article_path %></p>
